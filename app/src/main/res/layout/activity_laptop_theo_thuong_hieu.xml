<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.LaptopTheoThuongHieuActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarLaptopTheoThuongHieu"
        app:title="Laptop theo thương hiệu"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:titleTextColor="@color/white_text"
        android:background="@color/primary_color"/>
    <ListView
        android:id="@+id/listViewLaptopTheoThuongHieu"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </ListView>
</LinearLayout>